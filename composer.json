{
    "name": "radiatecode/laravel-route-permission",
    "description": "Laravel package to generate permissible routes based on permission middleware",
    "keywords": [
        "laravel-route-permission",
        "laravel-permission-view",
        "permission-view",
        "permissible-routes"
    ],
    "type": "library",
    "authors": [
        {
            "name": "radiatecode",
            "email": "radiate126@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RadiateCode\\LaravelRoutePermission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RadiateCode\\LaravelRoutePermission\\tests\\" : "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers" : [
                "RadiateCode\\LaravelRoutePermission\\PermissionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
